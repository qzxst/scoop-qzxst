    },
    "shortcuts": [
        [
            "LM Studio.exe",
            "LM Studio"
        ]
    ],
    "checkver": {
        "url": "https://lmstudio.ai/beta-releases",
        "regex": "Release Notes - LM Studio ([\\d.]+) Release Candidate (?<build>[\\d]+?)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://installers.lmstudio.ai/win32/x64/$version-$matchBuild/LM-Studio-$version-$matchBuild-x64.exe#/dl.7z"
            },
            "arm64": {
                "url": "https://installers.lmstudio.ai/win32/arm64/$version-$matchBuild/LM-Studio-$version-$matchBuild-arm64.exe#/dl.7z"
